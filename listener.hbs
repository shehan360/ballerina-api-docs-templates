<!DOCTYPE html>
<html>

{{> head }}

<body class="cBallerina-io">
    {{> bioBody }}
    <div class="row cBallerina-io-Gray-row">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 cBallerina-io-Home-Middle-col">

                    <nav class="cLeftNavContainer">
                        <ul class="cMainLeftNav">
                            <li class="sub-menu cTopicURL OpenUL">
                                <div class="cLeftMenuLink cTopiAtag">Listeners</div>
                                <ul class="sub-ul Open-submenu" style="display: none;">
                                    {{#each module.listeners}}
                                    <li {{#equals listener.name name}}class="active" {{/equals}}>
                                        <a href="{{name}}.html">{{name}}</a>
                                    </li>
                                    {{/each}}
                                </ul>
                            </li>
                            {{> currentModule }}
                        </ul>
                    </nav>
                </div>


                <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9 cBallerina-io-Home-Middle-col">
                    <div id="search-impl" class="search-bar"></div>

                    <div id="main" class="cBlallerina-io-docs-content-container">
                        <div class="wy-nav-content">
                            <div class="rst-content">
                                <div role="main">
                                    {{> breadcrumbhead }}
                                    <li>/</li>
                                    <li><a href="{{ rootPath }}{{module.id}}/index.html">{{module.id}}</a></li>
                                    <li>/</li>
                                    <li><a href="{{ rootPath }}{{module.id}}/index.html#listeners">Listeners</a></li>
                                    <li>/</li>{{ listener.name }}
                                    {{> breadcrumbfoot }}

                                    <div class="section">
                                        <h1>
                                            <span class="title-type">Listener - </span>
                                            {{#if project.isSingleFile}}
                                            {{project.sourceFileName}}
                                            {{else}}
                                            <a href="{{ rootPath }}{{module.id}}/index.html">{{module.id}}</a>
                                            {{/if}}
                                            : <span class="listeners">{{ listener.name }}</span>
                                        </h1>
                                        <p>{{{ listener.description }}}</p>

                                        <div class="constants">
                                            <div class="method-sum">
                                                {{#if listener.initMethod}}
                                                {{#listener.initMethod}}
                                                {{> initMethod }}
                                                {{/listener.initMethod}}
                                                {{/if}}

                                                {{#if listener.otherMethods}}
                                                <h2>Methods</h2>
                                                <div class="method-list">
                                                    {{#each listener.otherMethods}}
                                                    {{> methodSignature }}
                                                    {{/each}}
                                                </div>
                                                {{/if}}

                                                {{#if listener.lifeCycleMethods}}
                                                <h2>LifeCycle Methods</h2>
                                                <div class="method-list">
                                                    {{#each listener.lifeCycleMethods}}
                                                    {{> methodSignature }}
                                                    {{/each}}
                                                </div>
                                                {{/if}}
                                            </div>

                                            {{#if listener.fields}}
                                            {{#listener}}
                                            <h2>Fields</h2>
                                            {{> fields }}
                                            {{/listener}}
                                            {{/if}}

                                            {{#if listener.otherMethods }}
                                            <h2>Methods</h2>
                                            {{#each listener.otherMethods }}
                                            {{> method }}
                                            {{/each}}
                                            {{/if}}

                                            {{#if listener.lifeCycleMethods }}
                                            <h2>LifeCycle Methods</h2>
                                            {{#each listener.lifeCycleMethods }}
                                            {{> method }}
                                            {{/each}}
                                            {{/if}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{> foot }}
</body>

{{> footer }}