<!DOCTYPE html>
<html>

{{> head }}

<body class="cBallerina-io">
    {{> bioBody }}
    <div class="row cBallerina-io-Gray-row">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 cBallerina-io-Home-Middle-col">

                    <nav class="cLeftNavContainer">
                        <ul class="cMainLeftNav">
                            <li class="sub-menu cTopicURL OpenUL">
                                <div class="cLeftMenuLink cTopiAtag">Objects</div>
                                <ul class="sub-ul Open-submenu" style="display: none;">
                                    {{#each module.objects}}
                                    <li {{#equals object.name name}}class="active" {{/equals}}>
                                        <a href="{{name}}.html">{{name}}</a>
                                    </li>
                                    {{/each}}
                                </ul>
                            </li>
                            {{> currentModule }}
                        </ul>
                    </nav>
                </div>


                <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9 cBallerina-io-Home-Middle-col">
                    <div id="search-impl" class="search-bar"></div>

                    <div id="main" class="cBlallerina-io-docs-content-container">
                        <div class="wy-nav-content">
                            <div class="rst-content">
                                <div role="main">
                                    {{> breadcrumbhead }}
                                    <li>/</li><li><a href="{{ rootPath }}{{module.id}}/index.html">{{module.id}}</a></li>
                                    <li>/</li><li><a href="{{ rootPath }}{{module.id}}/index.html#objects">Objects</a></li>
                                    <li>/</li>{{ object.name }}
                                    {{> breadcrumbfoot }}

                                    <div class="section">
                                        <h1>
                                            <span class="title-type">Object - </span>
                                            {{#if project.isSingleFile}}
                                            {{project.sourceFileName}}
                                            {{else}}
                                            <a href="{{ rootPath }}{{module.id}}/index.html">{{module.id}}</a>
                                            {{/if}} :
                                            <span
                                                class="objects {{#if object.isDeprecated}}strike{{/if}}">{{ object.name }}</span>
                                        </h1>
                                        <p>
                                            {{#if object.isDeprecated}}
                                            <span class="stab">Deprecated</span>
                                            {{/if}}
                                            {{{ object.description }}}
                                        </p>

                                        <div class="constants">
                                            <div class="method-sum">
                                                {{#if object.initMethod}}
                                                {{#object.initMethod}}
                                                {{> initMethod }}
                                                {{/object.initMethod}}
                                                {{/if}}


                                                {{#if object.otherMethods}}
                                                <h2>Methods</h2>
                                                <div class="method-list">
                                                    {{#each object.otherMethods}}
                                                    {{> methodSignature }}
                                                    {{/each}}
                                                </div>
                                                {{/if}}

                                                {{#if object.fields}}
                                                {{#object}}
                                                <h2>Fields</h2>
                                                {{> fields }}
                                                {{/object}}
                                                {{/if}}
                                            </div>

                                            {{#if object.otherMethods }}
                                            {{#each object.otherMethods }}
                                            {{> method }}
                                            {{/each}}
                                            {{/if}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{> foot }}
</body>

{{> footer }}